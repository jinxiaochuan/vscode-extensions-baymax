{
  "name": "youzan-baymax",
  "displayName": "youzan-baymax",
  "description": "youzan baymax",
  "version": "0.0.5",
  "author": "jinxiaochuan",
  "license": "ISC",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jinxiaochuan/vscode-extensions-baymax.git"
  },
  "publisher": "jinxiaochuan",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onCommand:BaymaxGenerate",
    "onCommand:BaymaxExampleView",
    "onCommand:BaymaxDragView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      }
    ],
    "commands": [
      {
        "command": "BaymaxGenerate",
        "title": "Baymax: 生成代码"
      },
      {
        "command": "BaymaxExampleView",
        "title": "Baymax: 示例拖拽"
      },
      {
        "command": "BaymaxDragView",
        "title": "Baymax: 低代码拖拽"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "baymax",
          "title": "有赞云baymax模板代码",
          "icon": "images/bear.png"
        }
      ]
    },
    "views": {
      "baymax": [
        {
          "id": "ShowExampleView",
          "name": "拖拽小示例"
        },
        {
          "id": "ShowDragView",
          "name": "拖拽生成代码"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "BaymaxExampleView",
          "when": "view == ShowExampleView",
          "group": "navigation"
        },
        {
          "command": "BaymaxDragView",
          "when": "view == ShowDragView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run build",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "start:web-view": "cd ./webview && yarn start",
    "build:webview": "cd ./webview && yarn build-for-tool",
    "build": "yarn run package && yarn run build:webview"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "^2.5.12",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "cs-channel": "^1.0.4",
    "fs-extra": "^10.0.0",
    "node-fetch": "^2.6.1"
  },
  "bugs": {
    "url": "https://github.com/jinxiaochuan/vscode-extensions-baymax/issues"
  },
  "homepage": "https://github.com/jinxiaochuan/vscode-extensions-baymax#readme"
}
